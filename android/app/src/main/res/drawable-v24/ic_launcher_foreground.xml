<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">
    
    <!-- O ícone de primeiro plano deve ocupar a área central (72x72 dp) -->
    <group
        android:translateX="18"
        android:translateY="18">
        
        <!-- Gráfico de Barras e Seta (Roxo #8A2BE2) -->
        
        <!-- Barra 1 -->
        <path
            android:fillColor="#8A2BE2"
            android:pathData="M 15 70 L 15 50 L 25 50 L 25 70 Z" />
        <!-- Barra 2 -->
        <path
            android:fillColor="#8A2BE2"
            android:pathData="M 35 70 L 35 40 L 45 40 L 45 70 Z" />
        <!-- Barra 3 -->
        <path
            android:fillColor="#8A2BE2"
            android:pathData="M 55 70 L 55 30 L 65 30 L 65 70 Z" />
        
        <!-- Linha de tendência (simplificada) -->
        <path
            android:strokeColor="#8A2BE2"
            android:strokeWidth="4"
            android:strokeLineCap="round"
            android:strokeLineJoin="round"
            android:pathData="M 15 50 L 35 40 L 55 30" />
            
        <!-- Seta no topo da última barra -->
        <path
            android:fillColor="#8A2BE2"
            android:pathData="M 65 30 L 70 25 L 75 30 L 65 30 Z" />
            
    </group>
</vector>